<div class="col-md-4">

   <!-- Blog Search Well -->
   <div class="well">
       <h4>Blog Search</h4>
       <div class="input-group">
         <%= search_form_for @q, url: blogs_path do |f| %>
         <%= f.search_field :title_or_content_cont %>
         <%= f.submit %>
        <% end %>
       </div>
    </div>


    <div class="well">
        <h4>Latest Posts</h4>
        <% latst_posts.each do |post| %>
        <ul>
          <li><%= link_to post.title, blog_path(post) %></li>
        </ul>
        <% end %>
    </div>
</div>
