<h1>用户登录</h1>

<div class="row">
  <div class="col-sm-4">
    <%= form_tag admin_sessions_path, method: "post" do %>
      <div class="form-group">
      <%= text_field_tag :username, nil, class: "form-control", placeholder: "用户名..." %>
      </div>
      <div class="form-group">
        <%= password_field_tag :password, nil, class: "form-control", placeholder: "密码..." %>
      </div>
      <%= submit_tag "登录", class: "btn btn-primary" %>
     <% end %>
  </div>
  <div class="col-sm-8">
  </div>
</div>
